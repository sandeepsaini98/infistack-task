<% if @user.custom_fields.present? %>
  <h2>Custom Fields</h2>
  <ul>
    <% @user.custom_fields.each do |custom_field| %>
      <li>
        <strong><%= custom_field.field_name %>:</strong> <%= custom_field.default_value %> |
        <%= link_to 'Edit', edit_user_custom_field_path(@user, custom_field) %>
      </li>
    <% end %>
  </ul>
<% end %>
<%= link_to 'Add Custom Field', new_user_custom_field_path(@user)%> </br> |
<%= link_to 'Back', user_path(@user) %>
